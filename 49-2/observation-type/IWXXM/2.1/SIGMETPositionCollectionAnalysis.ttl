@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct:   <http://purl.org/dc/terms/> .
@prefix reg:   <http://purl.org/linked-data/registry#> .
@prefix ldp:   <http://www.w3.org/ns/ldp#> .

<SIGMETPositionCollectionAnalysis>
        a                skos:Concept ;
        rdfs:label       "SIGMET position collection analysis"@en ;
        dct:description  "SIGMETPositionCollectionAnalysis (a subclass of SamplingObservation from METCE) is intended  for use when reporting the forecast position of meteorological conditions hazardous to flight. The result of this observation type shall refer to one SIGMETPositionCollection which represents  the forecast positions of SIGMET phenomena. SIGMETPositionCollectionAnalysis enforces the following additional constraints: “featureOfInterest” shall refer to an entity of type SF_SamplingSurface and the associated “sampledFeature”must be an airspace."@en ;
        skos:notation    "SIGMETPositionCollectionAnalysis" ;
        skos:prefLabel   "SIGMET Position Collection Analysis" .